{
  "name": "@repo/ui",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --max-warnings 0 --fix",
    "gen": "turbo gen ui",
    "build:law": "set BRAND=law && rimraf dist && tsc -b -v",
    "build:geek": "set BRAND=geek && rimraf dist && tsc -b -v",
    "build:finance": "set BRAND=finance && rimraf dist && tsc -b -v",
    "storybook:finance": "set BRAND=finance && storybook dev -p 6006",
    "storybook:geek": "set BRAND=geek && storybook dev -p 6006",
    "storybook:law": "BRAND=law storybook dev -p 6006",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "dependencies": {
    "react": "^19.0.0",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4",
    "@repo/tokens": "*",
    "@repo/ds": "*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@repo/eslint-config": "*",
    "@repo/typescript": "*",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-webpack5": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.11.2",
    "storybook": "^8.4.7",
    "typescript": "5.5.4"
  }
}
